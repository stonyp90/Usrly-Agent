{
  "name": "agent-core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/agent-core/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/agent-core",
        "tsConfig": "libs/agent-core/tsconfig.lib.json",
        "packageJson": "libs/agent-core/package.json",
        "main": "libs/agent-core/src/index.ts",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/agent-core/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
