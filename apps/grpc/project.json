{
  "name": "grpc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/grpc/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/grpc",
        "main": "apps/grpc/src/main.ts",
        "tsConfig": "apps/grpc/tsconfig.app.json",
        "webpackConfig": "apps/grpc/webpack.config.js",
        "assets": [
          "apps/grpc/src/assets",
          {
            "glob": "**/*.proto",
            "input": "apps/grpc/src/proto",
            "output": "proto"
          }
        ],
        "generatePackageJson": true
      },
      "configurations": {
        "development": {
          "optimization": false,
          "sourceMap": true
        },
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "grpc:build",
        "watch": true
      },
      "configurations": {
        "development": {
          "buildTarget": "grpc:build:development"
        },
        "production": {
          "buildTarget": "grpc:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/grpc/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
